{
  "name": "@broken-harmony/playlist",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./dist/@main/index.js",
    "dev": "tsx watch ./src/@main/index.ts",
    "build": "tsc && tsc-alias",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit -p .",
    "pre-commit": "npm run typecheck"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@broken-harmony/eslint-config": "workspace:^",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.12.1",
    "typescript": "^5.4.5"
  }
}