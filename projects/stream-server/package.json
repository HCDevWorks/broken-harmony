{
  "name": "@youtube-livestream/stream-server",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./dist/@main/",
    "dev": "tsx watch ./src/@main/index.ts",
    "build": "tsc && tsc-alias",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit -p .",
    "pre-commit": "npm run typecheck"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "express": "^4.19.2",
    "ffmpeg-static": "^5.2.0",
    "googleapis": "^142.0.0",
    "node-gyp": "^10.1.0",
    "node-media-server": "^2.7.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.2",
    "@youtube-livestream/eslint-config": "workspace:^",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  }
}
